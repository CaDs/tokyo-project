<div class="container">
  <div class="row-fluid">
    <ul class="thumbnails">
      <% visions.in_groups_of(3) do |group| %>
        <% group.compact.each do |vision| %>
          <li class="span4">
            <%= partial('layouts/landing_block', :locals => {:vision => vision, :show_link => true})%>
          </li>
        <% end %>
      <% end %>
    </ul>
  </div>
</div>